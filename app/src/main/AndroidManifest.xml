<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.m.usama.groocerystore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GrooceryStore">

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".auth.signup.Registration"/>
        <activity android:name=".auth.login.Login" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Payment" />
        <activity android:name=".products.ProductsListActivity" />
        <activity android:name=".products.details.ProductDetailActivity" />
        <activity android:name=".cart.CartActivity" />


    </application>

</manifest>